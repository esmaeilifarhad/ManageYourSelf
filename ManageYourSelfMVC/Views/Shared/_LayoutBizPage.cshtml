            <div style="font-family: BNazanin;font-size: 12px;">
                <!--  Modal Master -->
                <div class="modal fade" id="MasterModal">
                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content">
                            <!-- Modal Header -->
                            <div class="modal-header" style="direction:ltr">
                                <br>
                                <h4 class="modal-title HeaderModal"></h4>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <!-- Modal body -->
                            <div class="modal-body BodyModal">
                            </div>
                            <!-- Modal footer -->
                            <div class="modal-footer">
                                <button type="button" class="btn btn-danger" data-dismiss="modal">بستن</button>
                                <span> </span>
                                <button type="button" class="btn btn-primary btnSave" data-dismiss="modal">ثبت</button>
                            </div>
                        </div>
                    </div>
                </div>
                <!--  Modal Show Example -->
                <div class="modal fade" id="myModalExa">
                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content">
                            <!-- Modal Header -->
                            <div class="modal-header" style="direction:ltr">
                                <br>
                                <h4 class="modal-title" id="HeaderExample"></h4>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <!-- Modal body -->
                            <div class="modal-body" id="EditHolder">
                            </div>
                            <!-- Modal footer -->
                            <div class="modal-footer">
                                <button type="button" class="btn btn-warning" data-dismiss="modal">بستن</button>
                                <span></span><span></span>
                                <button type="button" class="btn btn-success NewExample">مثال جدید</button>
                            </div>
                        </div>
                    </div>
                </div>
                <!--New Example-->
                <div class="modal fade" id="ModalNewExample">
                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content">
                            <!-- Modal Header -->
                            <div class="modal-header" style="direction:ltr">
                                <br>
                                <h4 class="modal-title"></h4>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <!-- Modal body -->
                            <div class="modal-body">
                            </div>
