<script>
    $("input[name='DicEng']").change(function () {
        var Word = $("#MasterModal .table input[name='DicEng']").val()
       // var Word = "freak";
        var urll = "/Dictionary/SerchWord?Word="+Word;
        $.ajax({
            type: "Get",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            url: urll,
            success: function (result) {
                // var obj = data;
                //var obj = JSON.parse(data);
                $(".SerchWord").html(result);
              //  $("#MasterModal").modal();

            }
        })
    });
</script>
