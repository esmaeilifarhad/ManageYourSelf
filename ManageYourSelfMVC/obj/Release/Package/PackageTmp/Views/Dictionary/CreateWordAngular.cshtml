        <tr>
            <td>انگلیسی</td>
            <td><input type='text' name='DicEng' ng-model="DicEng" /></td>
        </tr>
        <tr>
            <td>فارسی</td>
            <td><input type='text' name='DicFar' ng-model="DicFar" /></td>
        </tr>
        <tr>
            <td>فونتیک</td>
            <td><input type='text' name='DicPho' ng-model="DicPho" /></td>
