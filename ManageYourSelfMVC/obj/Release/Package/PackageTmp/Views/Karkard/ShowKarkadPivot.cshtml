@model  ManageYourSelfMVC.ViewModels.Karkard.VMKarkard
 <!--نمایش کارکرد-->
    <div id="ShowKarkard" class="table-responsive" style="font-family:BNazanin;font-size:11px">
        <table class="table table-dark table-striped" style="direction: ltr; text-align: center;">
            <thead>
                <tr>
                    
                    @foreach (System.Data.DataColumn column in Model.ShowKarkadPivotNotParamHeader.Columns)
                       {
                        <th>@column.ColumnName</th>
                    }
                </tr>
            </thead>
            <tbody>
                @foreach (System.Data.DataRow item in Model.ShowKarkadPivotNotParamHeader.Rows)
                {
                    <tr>
                        @for (int i = 0; i < Model.ShowKarkadPivotNotParamHeader.Columns.Count; i++)
                        {
                            <td>@item[i].ToString()</td>
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
